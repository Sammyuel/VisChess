{
  "name": "cli-ux",
  "description": "set of CLI output utilities",
  "version": "1.1.13",
  "author": "Jeff Dickey",
  "bugs": "https://github.com/jdxcode/cli-ux/issues",
  "dependencies": {
    "@heroku/linewrap": "^1.0.0",
    "ansi-escapes": "^3.0.0",
    "ansi-styles": "^3.2.0",
    "cardinal": "^1.0.0",
    "chalk": "^2.1.0",
    "fs-extra": "^4.0.2",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "password-prompt": "^1.0.3",
    "strip-ansi": "^4.0.0",
    "supports-color": "^4.4.0",
    "ts-lodash": "^4.0.5"
  },
  "devDependencies": {
    "@types/ansi-styles": "^2.0.30",
    "@types/chalk": "^0.4.31",
    "@types/fs-extra": "^4.0.2",
    "@types/jest": "^20.0.8",
    "@types/lodash": "^4.14.74",
    "@types/node": "^8.0.28",
    "@types/strip-ansi": "^3.0.0",
    "@types/supports-color": "^3.1.0",
    "babel-core": "^6.26.0",
    "del-cli": "^1.1.0",
    "husky": "^0.14.3",
    "jest": "^21.1.0",
    "lint-staged": "^4.2.1",
    "prettier": "^1.7.0",
    "remap-istanbul": "^0.9.5",
    "rimraf": "^2.6.2",
    "std-mocks": "^1.0.1",
    "ts-jest": "^21.0.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.2"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/jdxcode/cli-ux",
  "jest": {
    "coverageReporters": [
      "json"
    ],
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ]
  },
  "keywords": [
    "cli"
  ],
  "license": "ISC",
  "lint-staged": {
    "**/*.ts": [
      "prettier --write",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "main": "./lib/index.js",
  "repository": "jdxcode/cli-ux",
  "scripts": {
    "coverage": "cat coverage/coverage-final.json | remap-istanbul -o coverage/coverage-final.json && curl -s https://codecov.io/bash | bash",
    "precommit": "lint-staged",
    "prepare": "del lib && tsc",
    "pretest": "tsc --sourcemap",
    "test": "jest"
  },
  "types": "./lib/index.d.ts"
}
