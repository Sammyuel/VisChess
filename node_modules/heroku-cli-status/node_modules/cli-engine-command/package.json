{
  "name": "cli-engine-command",
  "description": "base CLI command for cli-engine",
  "version": "7.0.6",
  "author": "Jeff Dickey @dickeyxxx",
  "bugs": "https://github.com/heroku/cli-engine-command/issues",
  "dependencies": {
    "ansi-escapes": "2.x",
    "cardinal": "1.x",
    "chalk": "^1.0.0",
    "fs-extra": "^4.0.1",
    "http-call": "^3.0.1",
    "lodash.ary": "^4.1.1",
    "lodash.defaults": "^4.2.0",
    "lodash.get": "^4.4.2",
    "lodash.identity": "^3.0.0",
    "lodash.keys": "^4.2.0",
    "lodash.maxby": "4.x",
    "lodash.merge": "4.x",
    "lodash.partial": "^4.2.1",
    "lodash.property": "^4.4.2",
    "lodash.result": "^4.5.2",
    "moment": "^2.18.1",
    "string": "3.x",
    "supports-color": "^4.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "cli-engine-config": "^2.0.0",
    "eslint": "^4.6.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "20.0.3",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "flow-bin": "^0.54.0",
    "flow-copy-source": "^1.2.1",
    "flow-typed": "^2.1.5",
    "husky": "^0.14.3",
    "jest": "20.0.4",
    "jest-junit": "^3.0.0",
    "lint-staged": "^4.0.4",
    "nock": "^9.0.14",
    "nodemon": "1.11.0",
    "rimraf": "2.6.1",
    "std-mocks": "1.0.1"
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/heroku/cli-engine-command",
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./src",
    "coveragePathIgnorePatterns": [
      "src/output/linewrap.js"
    ],
    "setupFiles": [
      "../test/init.js"
    ]
  },
  "keywords": [
    "heroku",
    "heroku-cli-plugin"
  ],
  "license": "ISC",
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "peerDependencies": {
    "cli-engine-config": "^2.0.0"
  },
  "repository": "heroku/cli-engine-command",
  "scripts": {
    "build": "babel src -d lib --ignore '*.test.js'",
    "clean": "rimraf lib",
    "copy-flow": "flow-copy-source -v -i '**/*.test.js' src lib",
    "precommit": "lint-staged",
    "prepare": "npm run clean && npm run build && npm run copy-flow",
    "test": "jest && flow && eslint .",
    "watch": "nodemon -e js -i lib --exec \"npm run prepare\""
  }
}
